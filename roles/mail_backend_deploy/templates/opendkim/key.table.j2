# OpenDKIM key table
# Generated by Ansible - do not edit manually
# Role: mail_backend_deploy
#
# Maps key names to signing domains and key files
# Format: key-name domain:selector:key-file

{% for domain in mail_backend_domains %}
{{ mail_backend_dkim_selector }}._domainkey.{{ domain }}    {{ domain }}:{{ mail_backend_dkim_selector }}:{{ mail_backend_dkim_key_path }}/{{ domain }}/{{ mail_backend_dkim_selector }}.private
{% endfor %}
