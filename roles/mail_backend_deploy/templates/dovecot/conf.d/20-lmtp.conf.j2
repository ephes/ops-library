# Dovecot LMTP configuration
# Generated by Ansible - do not edit manually
# Role: mail_backend_deploy

# LMTP settings
protocol lmtp {
  # Plugins to load
  mail_plugins = $mail_plugins{% if mail_backend_sieve_enabled %} sieve{% endif %}


  # Postmaster address for bounce messages
  postmaster_address = postmaster@{{ mail_backend_mydomain }}
}
